#ifndef IN_DATA_H
#define IN_DATA_H

#include <stdint.h>

typedef uint8_t u8;

// Image data with header (Little Endian format)
// Structure: [Height 4B][Width 4B][Pixel Data]
u8 raw_image_file[] __attribute__ ((aligned (32))) = {
    // --- HEADER (8 Bytes) ---
    // Height = 10
    0x0A, 0x00, 0x00, 0x00,
    // Width = 20
    0x14, 0x00, 0x00, 0x00,

