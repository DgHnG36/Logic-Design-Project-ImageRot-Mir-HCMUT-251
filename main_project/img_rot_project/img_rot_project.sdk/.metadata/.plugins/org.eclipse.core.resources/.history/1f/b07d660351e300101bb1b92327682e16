#ifndef IN_DATA_H
#define IN_DATA_H

#include <stdint.h>

typedef uint8_t u8;

// Image data with header (Little Endian format)
// Structure: [Height 4B][Width 4B][Pixel Data]
u8 raw_image_file[] __attribute__ ((aligned (32))) = {
    // --- HEADER (8 Bytes) ---
    // Height = 2
    0x02, 0x00, 0x00, 0x00,
    // Width = 4
    0x04, 0x00, 0x00, 0x00,

    // --- PIXEL DATA ---
    0x00, 0x55, 0xAA, 0xFF,
    0x00, 0x55, 0xAA, 0xFF
};

#endif // IN_DATA_H
