#ifndef IN_DATA_H
#define IN_DATA_H

#include <stdint.h>

typedef uint8_t u8;

// Image data with header (Little Endian format)
// Structure: [Height 4B][Width 4B][Pixel Data]
u8 raw_image_file[] __attribute__ ((aligned (32))) = {
    // --- HEADER (8 Bytes) ---
    // Height = 8
    0x08, 0x00, 0x00, 0x00,
    // Width = 8
    0x08, 0x00, 0x00, 0x00,

    // --- PIXEL DATA ---
    0x00, 0x24, 0x48, 0x6D, 0x91, 0xB6, 0xDA, 0xFF,
    0x00, 0x24, 0x48, 0x6D, 0x91, 0xB6, 0xDA, 0xFF,
    0x00, 0x24, 0x48, 0x6D, 0x91, 0xB6, 0xDA, 0xFF,
    0x00, 0x24, 0x48, 0x6D, 0x91, 0xB6, 0xDA, 0xFF,
    0x00, 0x24, 0x48, 0x6D, 0x91, 0xB6, 0xDA, 0xFF,
    0x00, 0x24, 0x48, 0x6D, 0x91, 0xB6, 0xDA, 0xFF,
    0x00, 0x24, 0x48, 0x6D, 0x91, 0xB6, 0xDA, 0xFF,
    0x00, 0x24, 0x48, 0x6D, 0x91, 0xB6, 0xDA, 0xFF
};

#endif // IN_DATA_H
